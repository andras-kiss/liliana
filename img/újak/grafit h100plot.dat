set samples 200
set isosamples 200
set pm3d map
set pm3d interpolate 4,4
set dgrid 41,31,10 gauss 80,80
set xlabel "X / {/Symbol m}m"
set ylabel "Y / {/Symbol m}m"
set cblabel "{/Symbol m}A/cm^2"
set palette rgbformulae -33, -13, -10
set cbrange [-400:600]
set label "C" at 100,2800 tc rgb "black" font ",30" front
set size ratio 0.66666
set size 0.8,0.625
set term postscript enhanced color
set out "grafit_h_100.eps"
splot "grafit_h100_field.dat" u ($1):($2):(($7-25)*(10)) notitle
