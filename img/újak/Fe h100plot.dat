set samples 200
set isosamples 200
set pm3d map
set pm3d interpolate 4,4
set dgrid 31,21,10 gauss 80,80
set xlabel "X / {/Symbol m}m"
set ylabel "Y / {/Symbol m}m"
set cblabel "{/Symbol m}A/cm^2"
set cbrange [-300:-1200]
set palette rgbformulae -33, -13, -10
set label "A" at 100,1800 tc rgb "black" font ",30" front
set size ratio 0.66666
set size 0.8,0.625
set term postscript enhanced color
set out "Fe_h100.eps"
splot "Fe_h100_field.dat" u ($1):($2):(($7+25)*(-10)) notitle
